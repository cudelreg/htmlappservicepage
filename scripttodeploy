#deploy via Powershell
$templateFile = "https://github.com/cudelreg/htmlappservicepage/ARMtemplate"
New-AzResourceGroupDeployment `
  -Name CurtARMtemplate `
  -ResourceGroupName TestCurt-ARM `
  -TemplateFile $templateFile 


#deploy via AZ CLI
templateFile="https://github.com/cudelreg/htmlappservicepage/ARMtemplate"
az deployment group create \
  --name CurtARMtemplate \
  --resource-group TestCurt-ARM \
  --template-file $templateFile


